{
	"moduleDirectories": [
		  "node_modules",
		  "src"
	  ],
	"setupFiles": [
		  "<rootDir>/src/setupTests.js"
	  ],
	"coverageReporters": [
		  "lcov",
		  "html",
		  "text"
	  ],
	"coveragePathIgnorePatterns": [
		  "/node_modules/",
		  "/jest"
	  ],
	  "transform": {
		  "^.+\\.jsx?$": "babel-jest",
		  "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/src/assetsTransformer.js"
	  }
  }